{
    "apc.font.family": "Cascadia Code",
    "breadcrumbs.enabled": false,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.fontFamily": "Cascadia Code",
    "editor.fontLigatures": true,
    "editor.minimap.enabled": false,
    "editor.renderLineHighlight": "gutter",
    "editor.rulers": [80, 120],
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "editor.semanticHighlighting.enabled": false,
    "explorer.compactFolders": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.fileNesting.enabled": true,
    "terminal.integrated.smoothScrolling": true,
    "window.commandCenter": false,
    "window.density.editorTabHeight": "compact",
    "window.titleBarStyle": "native",
    "workbench.activityBar.location": "hidden",
    "workbench.colorTheme": "Omni",
    "workbench.editor.labelFormat": "short",
    "workbench.iconTheme": "symbols",
    "workbench.layoutControl.enabled": false,
    "workbench.reduceMotion": "off",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.statusBar.visible": false,
    "workbench.tree.indent": 18,
    "workbench.editorAssociations": {
        "{git,gitlens}:/**/*.{md,csv,svg}": "default"
    },
    "markdown.preview.openPreview": "never",
    "workbench.editor.doubleClickTabToToggleEditorGroupSizes": "off",
    "vscode-office.editorTheme": "Auto",
    "vscode-office.openOutline": false,
    "workbench.editor.defaultBinaryEditor": "default",
    "workbench.editor.enablePreview": false,
    "workbench.editorAssociations": {
        "*.md": "default"
    }
}
